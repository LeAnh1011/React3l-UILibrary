{
  "homepage": "http://leanh1011.github.io/React3l-UILibrary",
  "name": "react3l-ui-library",
  "sideEffects": false,
  "version": "0.2.29",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "files": [
    "build"
  ],
  "types": "./build/esm/index.d.ts",
  "typings": "./build/esm/index.d.ts",
  "type": "module",
  "dependencies": {
    "react-dropzone": "^11.5.1",
    "react-image-crop": "^9.0.5",
    "react-infinite-scroll-component": "^6.1.0"
  },
  "scripts": {
    "bundle-style": "sass ./src/styles/index.scss ./build/react3l.min.css --style compressed",
    "prebuild": "rimraf build",
    "build": "rollup -c && yarn bundle-style",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "publish-package": "npm version patch && npm publish",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.+(js|json|ts|tsx)": [
      "yarn lint"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "off",
      "import/no-anonymous-default-export": "off",
      "@typescript-eslint/no-unused-vars": "error",
      "no-console": "warn",
      "no-debugger": "error"
    },
    "ignorePatterns": [
      "build/**/*",
      "src/**/*.test.{ts,tsx}"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@carbon/icons-react": "^11.19.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/node-logger": "^6.3.6",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.6",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/carbon__icons-react": "^11.16.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.185",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^8.3.4",
    "ahooks": "^2.10.12",
    "antd": "4.24.8",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-react": "^7.31.0",
    "gh-pages": "^3.2.3",
    "husky": "^8.0.1",
    "i18next": "^20.3.5",
    "lint-staged": "^13.0.3",
    "postcss": "^8.3.6",
    "postcss-nesting": "10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react3l-advanced-filters": "^6.1.0-rc.1",
    "react3l-axios-observable": "^6.0.0",
    "react3l-common": "^6.0.0",
    "react3l-decorators": "^6.0.0",
    "react3l-localization": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rollup": "3.29.4",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-sass": "^1.2.10",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rxjs": "^7.3.0",
    "sass": "^1.49.9",
    "sass-loader": "^11.1.1",
    "ts-nameof.macro": "^4.2.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.1.2",
    "typescript-transform-paths": "^3.3.1"
  },
  "peerDependencies": {
    "@carbon/icons-react": "^11.17.0",
    "ahooks": "^2.10.12",
    "antd": "^4.16.9",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "5.2.0",
    "react3l-advanced-filters": "^6.1.0-rc.1",
    "react3l-axios-observable": "^6.0.0",
    "react3l-common": "^6.0.0",
    "react3l-decorators": "^6.0.0",
    "react3l-localization": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0"
  },
  "license": "MIT"
}
